class ClassA{
	string str1;
	int num1;
	
	void func1(){
		string s = "as if it's a non-static func";
	}
}


class ClassB extends ClassA{
	ClassA obj1 = new ClassA();
	string str2;
	int num2;
	
	void func2(){
		string s;
	}

	
}

class ClassC extends ClassB{

}

class ClassD extends ClassC{
	ClassA obj4 = new ClassA();
	
	
	void main(string[] args){
		obj4.func2();
		int i = 1;
	}
}