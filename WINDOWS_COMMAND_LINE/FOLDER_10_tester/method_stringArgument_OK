class Father
{
	string fatherString;
}

class Main extends Father
{
	string field1;
	
	void voidFuncWithStringArgument(string strArg1)
	{
		string strVar1 = strArg1;
	}

	string stringFuncWithStringArgument(string strArg1)
	{
		if (strArg1 == "Return me!")
		{
			return strArg1;
		}
		if (strArg1 != "Return me!")
		{
			string result = "Return me...";
			return result;
		}
		
		// This will never happen
		return "Why didn't you return me?";
	}

	void callVoidFunc()
	{
		string i;
		string j = i;
		
		voidFuncWithStringArgument(i);
		voidFuncWithStringArgument(j);
		voidFuncWithStringArgument(field1);
	}
	
	void callStringFunc(string[] args)
	{
		args[0] = stringFuncWithStringArgument(args[1]);
		string strVar = stringFuncWithStringArgument("wassap?");
		field1 = stringFuncWithStringArgument(fatherString);
	}

	void main(string[] args)
	{
		callVoidFunc();
		callStringFunc(args);
	}
}