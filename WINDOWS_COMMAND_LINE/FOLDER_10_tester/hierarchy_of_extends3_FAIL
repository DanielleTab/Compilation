class ClassA{
	string str1;
	int num1;
	
	void func1(){
		string s = "as if it's a non-static func";
	}
}


class ClassB extends ClassA{
	ClassA obj1;
	string str2;
	int num2;
	
	void assignerB()
	{
		obj1 = new ClassA();
	}
}

class ClassC extends ClassB{

}

class ClassD extends ClassC{
	ClassA obj4;
	int str4;
	
	void assignerD()
	{
		obj4 = new ClassA();
		str4 = obj4.num2;
	}
	
	
	void main(string[] args){
		int i = 1;
	}
}