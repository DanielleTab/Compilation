class PRINT
{
void printInt(int i)
{
}
}

class ReturnClass
{
	int returnFunc(int n)
	{
		if(n==0)
		{
			return 0;
		}
		if(n==1)
		{
			return 1;
		}
	
		return 2;
	}

}
class MainClass
{
	void main(string[] args)
	{
		PRINT p;
		ReturnClass ret = new ReturnClass();
	
		p.printInt(ret.returnFunc(0));
		p.printInt(ret.returnFunc(1));
		p.printInt(ret.returnFunc(2));
	}
}