class PRINT 
{
	void printInt(int i) 
	{
	}
}

class Rectest
{
	int b;

	void prevb()
	{
		PRINT p;
		if(b>0)
		{
			p.printInt(b);
			b=b-1;
			prevb();
		}
		PRINT ps;
		int j=0;
		ps.printInt(j);
	}
}

class Mainclass
{
	void main(string[] args)
	{
	
		Rectest r=new Rectest();
		r.b=12;
		r.prevb();
	}
}