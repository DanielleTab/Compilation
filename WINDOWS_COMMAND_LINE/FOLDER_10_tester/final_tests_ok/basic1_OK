class Print 
{
	void printInt(int i) 
	{
	}
}

class Rectest
{
	int b;
	Print p;
	void ctor()
	{
	p=new Print();
		b=3;
	}
	void prevb()
	{
		if(b>0)
		{
			p.printInt(b);
			b=b-1;
			prevb();
		}
		p.printInt(0);
	}
}

class Mainclass
{
	void main(string[] args)
	{
		Rectest p=new Rectest();
		p.ctor();
		p.prevb();
	}
}